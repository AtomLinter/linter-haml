{
  "name": "linter-haml",
  "activationCommands": {},
  "main": "./lib/main",
  "version": "1.2.0",
  "description": "Atom linter plugin for HAML, using haml-lint",
  "repository": "https://github.com/AtomLinter/linter-haml",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "^4.6.1",
    "atom-package-deps": "^4.0.1",
    "fs-extra": "0.26.6",
    "temp": "0.8.3",
    "xregexp": "3.1.0"
  },
  "package-deps": [
    "linter",
    "language-haml"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "devDependencies": {
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.1.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ]
    },
    "extends": "airbnb/base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
